    field(SCAN, "I/O Intr")
    field(FLNK, "$(P):RUNSTATE_STR")
}

record(mbbi, "$(P):RUNSTATE_STR")
{
    field(INP,  "$(P):RUNSTATE")
    field(ZRVL, 0)
    field(ZRST, "PROCESSING")
    field(ONVL, 1)
    field(ONST, "SETUP")
    field(TWVL, 2)
    field(TWST, "RUNNING")
    field(THVL, 3)
    field(THST, "PAUSED")
    field(FRVL, 4)
    field(FRST, "WAITING")
    field(FVVL, 5)
    field(FVST, "VETOING")
    field(SXVL, 6)
    field(SXST, "ENDING")
    field(SVVL, 7)
    field(SVST, "SAVING")
}

